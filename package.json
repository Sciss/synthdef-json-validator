    "lodash.values": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-power-assert": "^1.0.0",
    "eater": "^3.0.0-3",
    "eatest": "^0.4.0",
    "eslint": "^2.13.1",
    "eslint-plugin-node": "^1.5.0",
    "npm-run-all": "^2.2.0",
    "nyc": "^7.0.0",
    "power-assert": "^1.4.1"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "files": [
    "package.json",
    "README.md",
    "src",
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/synthdef-json-validator/",
  "keywords": [
